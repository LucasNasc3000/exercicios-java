import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);	
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Quantas quartos ser√£o alugados? ");
		int n = sc.nextInt();
		String[] estudantesParcial = new String[n];
		String[] estudantes = new String[n];
		int position = 0;
		
	    for(int i=0; i<n; i++) {
	    	System.out.println("#Rent"+i+1);
	    	System.out.println("Nome: ");
	    	estudantesParcial[i] = sc.next();
	    	System.out.println("Email: ");
	    	estudantesParcial[i] = sc.next();
	    	System.out.println("Quarto: ");
	    	position = sc.nextInt();
	    	for(int i2=0; i<estudantesParcial.length; i++) {
	    		estudantes[position] = estudantesParcial[i2];
	    	}
	    }
	    
	    for(int i=0; i<n; i++) {
	    	System.out.println(estudantes[i]);
	    }
		
		sc.close();
	}
	

}
