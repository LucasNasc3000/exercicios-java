import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);	
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Quantas quartos ser√£o alugados? ");
		int n = sc.nextInt();
		int position;
		int configPosition;
		String nome;
		String email;
		String[] estudantes;
		
	    for(int i=0; i<n; i++) {
	    	position = sc.nextInt();
	    	configPosition = sc.nextInt();
	    	
	    	System.out.printf("%d%n %d", position, configPosition);
	    }
		
		sc.close();
	}
	

}
